{
  "name": "NicoTunes",
  "version": "0.1.0",
  "description": "Niconico Music Player",
  "main": "app/js/main.js",
  "scripts": {
    "init": "gulp js css html",
    "start": "electron ./",
    "dist": "build --platform darwin --platform win32 --platform linux --arch all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/githayu/nicotunes.git"
  },
  "bugs": {
    "url": "https://github.com/githayu/nicotunes/issues"
  },
  "author": "Hayu",
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2017": "^1.3.1",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.3.13",
    "electron-builder": "^3.9.0",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^0.37.5",
    "gulp": "^3.8.11",
    "gulp-babel": "^6.1.1",
    "gulp-changed": "^1.3.0",
    "gulp-compass": "^2.0.4",
    "gulp-htmlmin": "^1.3.0",
    "gulp-pleeease": "^2.0.2",
    "gulp-plumber": "^1.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-watch": "^4.2.4"
  },
  "build": {
    "app-bundle-id": "net.nanoway.nicotunes",
    "osx": {
      "title": "NicoTunes",
      "icon": "build/icon.icns",
      "background": "build/background.png",
      "icon-size": "80",
      "contents": [
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 220,
          "type": "file"
        }
      ]
    },
    "win": {
      "title": "NicoTunes",
      "icon": "build/icon.ico"
    }
  }
}
